{
  "name": "noflow",
  "version": "0.6.0",
  "description": "A minimal middleware composer for the future.",
  "main": "lib/index.js",
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ysmood/noflow"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "api",
    "app",
    "router",
    "web",
    "promise",
    "async",
    "await",
    "es7"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/noflow/issues"
  },
  "homepage": "https://github.com/ysmood/noflow",
  "dependencies": {
    "yaku": "0.11.4"
  },
  "devDependencies": {
    "babel": "5.8.29",
    "babel-eslint": "4.1.3",
    "body-parser": "1.14.1",
    "eslint": "1.7.3",
    "junit": "0.8.2",
    "nokit": "0.15.3",
    "path-to-regexp": "1.2.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "double"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }
}
